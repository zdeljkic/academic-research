<?xml version='1.0'?>
<!DOCTYPE EntitySchema>
<entity name="book" longName="Book" color="#00ff80" pngImage="image.png" svgImage="image.svg"><!-- Format color : #RRGGBB where R,G and B are hexa digits -->
  <description>book</description>
  <fieldList>
    <field name="value" longName="Title" description="title" default="" level="default" format="string" ioMode="input">
      <string regex=".*"/>
    </field>
    <field name="authors" longName="Authors" description="authors" default="" optional="true" level="advanced" format="string" ioMode="input">
      <string regex=".*"/>
    </field>
    <field name="date" longName="Date" description="date" default="" optional="true" level="advanced" format="string" ioMode="input">
      <string regex=".*"/>
    </field>
  </fieldList>
</entity>
